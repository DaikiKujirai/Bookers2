  <%= render 'list', book_new:Book.new %>
    <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <%= link_to user_path(@user) do %>
            <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image" %></td>
            <% end %>
            <td><%= link_to user.name, user_path(user.id) %></td>
            <td><%= link_to 'Show', user_path(user) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<div class="container mt-5">
  <div class="row">
    <div class="mx-auto">
      <p>CopyRight infratop.inc</p>
    </div>
  </div>
</div>