<h1>User index</h1>
 <%= render 'list', book_new:Book.new %>
    <h1>Users</h1>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image" %></td>
          <td><%= link_to user.name, user_path(user.id) %></td>
          <td><%= user.introduction %></td>
          <td><%= link_to 'Show', user_path(user) %></td>
        </tr>
        <% end %>
      </tbody>


    </table>